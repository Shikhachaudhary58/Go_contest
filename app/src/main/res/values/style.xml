<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="rounded">
        <item name="cornerSize">50%</item>
    </style>
    <style name="AppTheme" parent="Theme.AppCompat.Light">
        <item name="colorPrimary">@color/design_default_color_primary</item>
        <item name="colorPrimaryDark">@color/design_default_color_primary_dark</item>
        <item name="colorAccent">@color/white</item>
    </style>
    <style name="TextAppearance.AppCompat.widget.ActionBar.Title" parent="@android:style/TextAppearance">
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="CustomRadioButtonStyle" parent="@android:style/Widget.CompoundButton.RadioButton">
        <item name="android:buttonTint">@color/customRadioButtonColor</item>
    </style>
    <!-- res/values/styles.xml -->

    <style name="SearchViewStyle" parent="Widget.AppCompat.SearchView">
        <!-- Customize the background color here -->
        <item name="android:background">@color/your_color_here</item>
    </style>


</resources>